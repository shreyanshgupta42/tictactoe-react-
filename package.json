        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "babel-preset-nano-react-app": "^0.1.0",
        "eslint": "^7.26.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.23.2",
        "parcel": "1.12.4",
        "prettier": "^2.3.0",
        "rimraf": "^3.0.2"
    },
    "babel": {
